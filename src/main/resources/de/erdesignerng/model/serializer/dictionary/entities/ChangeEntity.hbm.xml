<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="de.erdesignerng.model.serializer.dictionary.entities"
	default-lazy="false">

	<class name="ChangeEntity" table="mogdict_change" select-before-update="true">

		<id name="id" >
			<generator class="native" />
		</id>
		<version name="version"/>
		
		<property name="systemId" not-null="true"/>		
		
		<property name="creationUser" not-null="true"/>
		<property name="creationDate" not-null="true"/>
		<property name="lastModificationUser" not-null="false"/>
		<property name="lastModificationDate" not-null="false"/>

		<list name="statements" table="mogdict_change_stm" cascade="all-delete-orphan">
			<key column="changeid" foreign-key="mogdict_change_stm_fk1" not-null="true"/>
			<list-index column="listindex"/>
			<element column="stm" type="text" not-null="true"/>
		</list>
	</class>
</hibernate-mapping>